{
  "name": "PushDialer",
  "version": "1.0",
  "minimum_chrome_version": "6",
  "description": "Provides a context menu item for dialing phone numbers on your iPhone.",
  "icons": { "16": "Icons/16.png",
             "48": "Icons/48.png",
            "128": "Icons/128.png" },
  "permissions": [ "contextMenus", "tabs" ],
  "background_page": "Background.html",
  "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["Content.js"]
      }
    ]
}
